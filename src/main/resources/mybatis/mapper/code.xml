<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.kongkong.code.dao.ICodeDao">
 <select id="getCodeListByParent" parameterType="String" resultType="com.kongkong.code.vo.CodeVO">
		 SELECT comm_cd        
		       , comm_nm      
		       , comm_parent  
		       , comm_ord     
		    FROM comm_code        
		   where comm_parent = #{parentCode}
		   ORDER BY comm_ord DESC
 </select>
 </mapper>