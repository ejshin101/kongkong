                        function Mymileage(){
                     		$.ajax({
                    			type :"GET",
                    			url : "<c:url value='/event/mileagecheck'/>",	
                    			dataType : 'json',
                    			data : {
                    				memId:'${sessionScope.USER_INFO.userId}',
                    				requiredMileage: 0
                    			},		
                    			success :function(data){
                    				if(data.result){
                    					curMileage = data.curMileage;
                    					//alert(curMileage);
                    					$("#Mymileage").html("내 마일리지 : " + curMileage); 
                    				}
                    			}
                    			});  // mileage check ajax --end
                     		}
                        
                        Mymileage();
                        </script>
                        	<a id="Mymileage" ></a>
