<style type="text/css">
	button {border: 0px;}
	
	.kongkongs {
		display: inline-block;
	    padding: 13px 47px;
	    box-shadow: 0px 12px 20px 0px rgb(255 126 95 / 15%);
	    font-size: 14px;
	    color: #fff;
	    text-transform: capitalize;
	    -webkit-transition: 0.5s;
	    transition: 0.5s;
	    background-size: 300% 100%;
	}
	.kongkongs:hover {
	  color: #fff;
	  background-size: 100% 100%;
	}
	.kongkongs {
		background-image: -webkit-linear-gradient(222deg, #2E64FE, #2E2EFE, #5858FA, #2E64FE);
	    background-image: -o-linear-gradient(222deg, #2E64FE, #2E2EFE, #5858FA, #2E64FE);
	    background-image: linear-gradient(222deg, #2E64FE, #2E2EFE, #5858FA, #2E64FE);
	}
</style>

