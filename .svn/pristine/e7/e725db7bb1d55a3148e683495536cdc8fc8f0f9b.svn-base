<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kongkong.signup.dao.ISignUpDao">

	<select id="getMemberList" resultType="com.kongkong.member.vo.MemberVO">
		SELECT mem_id	
			 , mem_pass
			 , mem_name  	
		  FROM member			
	</select>
	<select id="getMember" resultType="com.kongkong.member.vo.MemberVO">
		SELECT mem_id	
			 , mem_pass
			 , mem_name  	
		  FROM member			
		 WHERE mem_id = #{gracePark}
	</select>
	<insert id="insertUser" >
	INSERT INTO member (
    	mem_id
      , mem_pass
      , mem_name
      , mem_bir
      , mem_zip
      , mem_add1
      , mem_add2
      , mem_hp
      , mem_mail
      , mem_delete
      , mem_role
	) VALUES (
    	#{memId}
      , #{memPass}
      , #{memName}
      , #{memBir}
      , #{memZip}
      , #{memAdd1}
      , #{memAdd2}
      , #{memHp}
      , #{memMail}
      , 'N'
      , 'normal'
	)
	</insert>
</mapper>