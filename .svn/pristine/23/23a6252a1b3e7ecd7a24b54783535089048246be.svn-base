<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kongkong.flight.dao.IFlightDao">
	<select id="getkName" resultType="String">
		select kname
		from airport_code
	</select>
	
	<select id="geteName" resultType="String">
		select kname
		from airport_code
	</select>
	
	<select id="getCode" parameterType="String" resultType="String">
		select code from airport_code 
		where ename= #{airportName}
  		   or kname= #{airportName}
	</select>
	
</mapper>