	}


	public String getBoSingoDate() {
		return boSingoDate;
	}


	public void setBoSingoDate(String boSingoDate) {
		this.boSingoDate = boSingoDate;
	}


	public String getBoBlockDate() {
		return boBlockDate;
	}


	public void setBoBlockDate(String boBlockDate) {
		this.boBlockDate = boBlockDate;
	}



	public String getBoBlock() {
		return boBlock;
	}



	public void setBoBlock(String boBlock) {
		this.boBlock = boBlock;
	}
	
	

}
