<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<%@ page session="false"%>
<%
	request.setCharacterEncoding("utf-8");
%>
<html>
<head>
<title>GoogleMap</title>
<style>
#google-map {
	width: 100%;
	height: 400px;
}
</style>
</head>
<body>
	<div id="google-map"></div>
	<script async defer
		src="https://maps.googleapis.com/maps/api/js?key=IzaSyD-kYmuKq38x7DnHMzYs8_rrcYEo0Eq8sw&callback=initMap">
	</script>
	<script>
		function initMap() {
			var lat, lon;
			console.log('Map is initialized.');
			var map = new google.maps.Map(
					document.getElementById('google-map'), {
						zoom : 4,
						center : {
							lat : 36.338192,
							lng : 127.393344
						}
					});
		}// initMap
	</script>
</body>
</html>